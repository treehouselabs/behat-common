{
  "name": "clippings/behat-common",
  "description": "Common Behat contexts to use in our Symfony projects",
  "license": "MIT",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1",
    "behat/behat": "^3.0",
    "behat/mink": "^1.6",
    "behat/mink-extension": "^2.0",
    "behat/mink-browserkit-driver": "^1.2",
    "behat/symfony2-extension": "^2.0",
    "doctrine/orm": "^2.2",
    "doctrine/common": "^2.2",
    "doctrine/data-fixtures": "^1.1",
    "phpunit/phpunit": "^5.7|^6|^7",
    "swiftmailer/swiftmailer": "^6.0",
    "symfony/dependency-injection": "^3.0",
    "symfony/http-kernel": "^3.0",
    "symfony/property-access": "^3.0",
    "symfony/security": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "TreeHouse\\BehatCommon\\": "src/TreeHouse/BehatCommon/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "replace": {
    "treehouselabs/behat-common": "self.version"
  }
}
